true t f = t
false t f = f

id x = x
const x u = x

isZero a = a (const false) true

pred n f x = (
  extract k = k id
  inc g h = h (g f)
  const u = x
  extract (n inc const)
)

a + b = (
  add an bn = a an (b an bn)
  add
)

a - b = b pred a

a <= b =
  if (isZero a) true
  else if (isZero b) false
  else ((pred a) <= (pred b))

5 <= 5
